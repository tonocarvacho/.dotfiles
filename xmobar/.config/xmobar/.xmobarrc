Config { font = "xft:Ubuntu-B::size=9:style=bold:antialias=true"
       , additionalFonts = ["xft:'fa-regular-400'::size=21:antialias=true"]
       , bgColor = "black"
       , fgColor = "#caa9fa"
       , alpha = 220
       , position = BottomSize L 100 26
       , textOffset = 18
       , textOffsets = [24, 24]
       , lowerOnStart = False
       , pickBroadest = False
       , persistent = False
       , hideOnStart = False
       , iconRoot = "."
       , allDesktops = False
       , commands = [                      
                    Run Date " %b %d %Y ∙ %H:%M " "date" 50
                    , Run Com ".config/xmobar/trayer-padding-icon.sh" [] "trayerpad" 20
                    , Run UnsafeStdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "}<fn=1>      \xf037</fn><fn=1>%UnsafeStdinReader%</fn>{<fc=#bd93f9>%date%</fc> "
       }
