Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
       , additionalFonts = []
       , bgColor = "black"
       , fgColor = "#caa9fa"
       --, alpha = 200
       , position = BottomSize L 100 16
       , lowerOnStart = False
       , pickBroadest = False
       , persistent = True
       , hideOnStart = False
       , iconRoot = "."
       , allDesktops = True
       , commands = [                      
                    Run Date "<fn=2>\xf017</fn>  %b %d %Y - %H:%M " "date" 50
                    , Run Com ".config/xmobar/trayer-padding-icon.sh" [] "trayerpad" 20
                    , Run StdinReader
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template = "  %StdinReader% }{ <fc=#caa9fa>%date%</fc> "
       }
